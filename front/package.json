{
  "name": "fs-manager-front",
  "productName": "FiharySoft Manager",
  "description": "Manager for FiharySoft (2.0)",
  "author": "FiharySoft SARL",
  "version": "2.1.2",
  "private": true,
  "homepage": "./",
  "main": "electron/index.js",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "add": "^2.0.6",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^3.6.0",
    "child_process": "^1.0.2",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "fast-levenshtein": "^3.0.0",
    "fuzzy": "^0.1.3",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "portfinder": "^1.0.28",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.3.0",
    "react-cropper": "^2.1.8",
    "react-date-picker": "^8.3.2",
    "react-datepicker": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-qr-code": "^2.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.7.7",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "remove": "^0.1.5",
    "sass": "^1.53.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.2.0",
    "typescript": "^5.1.6",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-pre": "react-scripts build && babel ./electron -d build --presets=@babel/preset-env --copy-files",
    "build-post": "electron-builder -w -c.extraMetadata.main=build/index.js",
    "build": "yarn build-pre && yarn build-post",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "concurrently \"set BROWSER=NONE && npm start\" \"wait-on http://localhost:7548 && electron .\""
  },
  "build": {
    "appId": "com.fiharysoft.manager",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "buildResources": "electron-assets"
    },
    "extraResources": [
      {
        "from": "./resources/extra",
        "to": "extra",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.0",
    "@types/fast-levenshtein": "0.0.2",

    "@types/react-router-dom": "^5.3.3",

    "@types/uuid": "^9.0.2",

    "concurrently": "^6.2.1",
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7",
    "wait-on": "^6.0.0",
    "yarn": "^1.22.11"
  }
}
